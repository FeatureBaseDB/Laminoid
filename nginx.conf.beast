user www-data;
worker_processes auto;
pid /run/nginx.pid;
events {
        worker_connections 768;
        # multi_accept on;
}
http {
        upstream beast {
                server localhost:8080;
        }
        server {
                listen  8088;
                location / {
                        auth_basic "beast";
                        auth_basic_user_file /etc/nginx/htpasswd;
                        proxy_pass http://beast/;
                }
        }
}